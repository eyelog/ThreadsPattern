<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.eyelog.threadspattern">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main_thread.ActivityMainThread"/>
        <activity android:name=".side_thread.ActivitySideThread"/>
        <activity android:name=".async_thread.ActivityAsyncThread"/>
        <activity android:name=".multi_side_thread.ActivityMultiThread"/>
        <activity android:name=".rx_thread.ActivityRxThread"/>
        <activity android:name=".rx_mvp_thread.ActivityRxMVPThread"/>
        <activity android:name=".rx_concat.ActivityRxConcatThread"/>
    </application>

</manifest>